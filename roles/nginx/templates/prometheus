server {
    listen 80 default_server;
    server_name _;

    location /prometheus {
        proxy_pass http://prometheus:9090/prometheus;  # Prometheus default port
    }
    
    location / {
        #deny all;
        proxy_set_header Host $http_host;
        proxy_pass http://Kryptograafia-2:{{ agama_port }};
    }
}

