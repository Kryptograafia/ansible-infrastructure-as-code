server {
    listen 80 default_server;
    server_name _;

    location / {
        uwsgi_pass 127.0.0.1:5000;
        include uwsgi_params;
    }
    
    location = /basic_status {
        stub_status;
    }

    location /grafana {
        proxy_set_header Host $http_host;
        proxy_pass http://Kryptograafia-1:3000;
    }
}

